public class Person {

  //string fields
  private String firstName;
  private String lastName;
  private String ID;

  //constructor
  public person(String firstName, String lastName, String ID){
    firstName = fname;
    lastName = lname;
    ID = id;
  }

//Accessor methods
  public String getFirstName(){
    return fname;
  }
  public String getLastName(){
    return lname;
  }
  public String getID(){
    return id;
  }

  //to string method
  public String toString(){
    return "Ther person's first name is " + fname + ", last name is " + lname + ", and id is " + id + ".";
  }
}
