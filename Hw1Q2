import java.util.LinkedList;

public class MyMain {

  public static void store (inputStream, LinkedList<person> list)
  {
    BufferedReader reader = new BufferedReader (newInputStreamReader (inputStream));
    string line;
    While ((line = reader.readLine() != null) 
    {
      string[] parts = line.split(",");
      list.add(new Person(fname, lname, id));
    }
  }

  public static void display (PrintStream outputStream, LinkedList<Person> list)
  {
    while  (list != null) 
    {
        outputStream.println(person);
    }
  }

  public static void find (String sid, LinkedList<Person> list) 
  {
    for (int i=0; i < list.size(); i++)
    {
      if (list.get(i).getID().equals(sid)) 
      {
        return i;
      }
    }
    return -1
  }




    
